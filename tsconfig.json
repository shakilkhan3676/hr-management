{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts",
        "app/_layout.jsx",
        "app/(home)/index.jsx",
        "nativewind-env.d.ts",
        "(tabs)/_layout.jsx",
        "constants/Colors.js",
        "components/HapticTab.jsx"
    ]
}
